@model TestPlansViewModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Last test executions</h2>

<table>
    <thead>
    <tr>
        <th>Id</th>
        <th>Test name</th>
        <th>Test result</th>
        <th> Detials</th>
        <th> Download</th>
        <th> Execution time</th>
    </tr>
    </thead>
    <tbody>
    <tr>
        <th>1</th>
        <th>Test name</th>
        <th>Passed</th>
        <th><a href="#">View Details</a></th>
        <th><a href="#">Download report</a></th>
        <th>@DateTime.Now.ToString("f") </th>
    </tr>
    <tr>
        <th>2</th>
        <th>Test name</th>
        <th>Fail</th>
        <th><a href="#">View Details</a></th>
        <th><a href="#">Download report</a></th>
        <th>@DateTime.Now.AddDays(-1).ToString("f") </th>
    </tr>
    </tbody>
</table>
